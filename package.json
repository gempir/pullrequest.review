    "clsx": "^2.1.1",
    "lucide-react": "^0.542.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-files-icons": "^0.7.0",
    "shiki": "^3.13.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.13",
    "wrangler": "^4.39.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.14",
    "@types/node": "^24.5.2",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.2",
    "tw-animate-css": "^1.4.0",
