{
  "name": "pullrequestdotreview",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev --mode dev",
    "tsc": "tsc --noEmit",
    "lint": "biome lint .",
    "format": "biome format --write .",
    "check": "biome check .",
    "build": "vite build && cp dist/client/_shell.html dist/client/index.html",
    "serve": "vite preview",
    "worker:dev": "wrangler dev --config wrangler.jsonc",
    "worker:preview": "wrangler dev --config wrangler.jsonc --remote",
    "cf-typegen": "wrangler types",
    "deploy": "bun run build && wrangler deploy --config wrangler.jsonc",
    "wrangler": "wrangler"
  },
  "dependencies": {
    "@pierre/diffs": "^1.0.10",
    "@radix-ui/react-collapsible": "^1.1.12",
