  "assets": {
    "directory": "./dist/client",
    "not_found_handling": "single-page-application"
  },
  "vars": {
    "SKIP_DEPENDENCY_INSTALL": "1"
  },
  "routes": [
    {
      "pattern": "pullrequest.review",
      "custom_domain": true
    }
  ]
}
